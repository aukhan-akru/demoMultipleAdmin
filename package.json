  "scripts": {
    "contracts": "node build_contracts",
    "start": "NODE_OPTIONS=--max-old-space-size=3000 node server.js",
    "develop": "npm run contracts && cross-env REACT_APP_SERVER=\"development\" react-scripts start HOST=0.0.0.0",
    "staging": "npm run contracts && cross-env REACT_APP_SERVER=\"staging\" react-scripts start HOST=0.0.0.0",
    "prototype": "npm run contracts && cross-env REACT_APP_SERVER=\"prototype\" react-scripts start HOST=0.0.0.0",
    "production": "npm run contracts && env-cmd -f .env.production react-scripts start HOST=0.0.0.0",
    "bscProd": "env-cmd -f .env.bsc_prod && npm run contracts && react-scripts start HOST=0.0.0.0",
    "build": "NODE_OPTIONS=--max-old-space-size=3000 GENERATE_SOURCEMAP=false react-scripts build && node server.js",
    "devDeploy": "npm run contracts && cross-env REACT_APP_SERVER=\"development\" npm run build",
    "stagDeploy": "npm run contracts && cross-env REACT_APP_SERVER=\"staging\" npm run build",
    "protoDeploy": "npm run contracts && cross-env REACT_APP_SERVER=\"prototype\" npm run build",
    "protoStart": "npm run contracts && cross-env REACT_APP_SERVER=\"prototype\" npm run start",
    "prodDeploy": "npm run contracts && env-cmd -f .env.production npm run build",
    "bscDeploy": "npm run contracts && env-cmd -f .env.bsc_dev npm run build",
    "avaxDeploy": "env-cmd -f .env.avax_dev npm run contracts && npm run build",
    "avaxProdDeploy": "env-cmd -f .env.avax_prod npm run contracts && npm run build",
    "bsc_dev": "env-cmd -f .env.bsc_dev npm run contracts && react-scripts start HOST=0.0.0.0",
    "avax_dev": "env-cmd -f .env.avax_dev react-scripts start HOST=0.0.0.0",
    "avax_prod": "env-cmd -f .env.avax_prod npm run contracts && react-scripts start HOST=0.0.0.0",
    "bsc_prod": "env-cmd -f .env.bsc_prod react-scripts start HOST=0.0.0.0",
    "bsc_prototype": "env-cmd -f .env.bsc_prototype && npm run contracts && react-scripts start HOST=0.0.0.0",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "test": "react-scripts test --env=jsdom",
    "bscprodDeploy": "env-cmd -f .env.bsc_prod npm run contracts && npm run build",
    "bscProtoDeploy": "env-cmd -f .env.bsc_prototype npm run contracts && npm run build",
    "scss": "node-sass src/styles/scss/style.scss src/styles/css/style.css",
    "scss:watch": "node-sass -w src/styles/scss/style.scss src/styles/css/style.css",
    "lint": "eslint \"**/*.+(js|jsx)\"",
    "lint:fix": "eslint --fix \"**/*.+(js|jsx)\"",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
